{
  "name": "pdfreader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\"",
    "dev:web": "next dev -p 3001",
    "dev:api": "cd backend && . .venv/bin/activate && python -m uvicorn main:app --reload --port 8000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stop": "pkill -f \"next dev\" && pkill -f \"uvicorn main:app\""
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/uuid": "^10.0.0",
    "@xenova/transformers": "^2.17.2",
    "concurrently": "^8.2.2",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
